@model ChatonsBDD_B31.Models.Vaccine

@{
    ViewData["Title"] = "Vaccins";
}

<h3>Etes-vous sûr de vouloir supprimer ce vaccin ?</h3>
<div>
    <h4>Vaccine</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.name)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.name)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.periodicity)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.periodicity)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Supprimer" class="btn btn-danger" /> |
        <a asp-action="Index">Retour à la liste</a>
    </form>
</div>
